peline {
    agent any

        stages {
	        stage('compile') {
		            steps {
			                    echo 'compiling stage'
					                    git 'https://github.com/kwameindevops/DevOpsClassCodes-aug.git'
							                    sh 'mvn compile'
									                    
											                }
													        }
														        stage('test') {
															            steps {
																                    echo 'testing stage'
																		                    sh 'mvn test'
																				                  
																						                  
																								              }
																									              }
																										              stage('package') {
																											                  steps {
																													                  echo 'package stage'
																															                  sh 'mvn package'
																																	                  
																																			                 
																																					             }
																																						             }
																																							             stage('Aqua-scan') {
																																								                 steps {
																																										                 echo 'Security'
																																												                 
																																														                 
																																																                
																																																		            }
																																																			            }
																																																				            stage('publish to NEUS') {
																																																					                steps {
																																																							                echo 'Store package in NEXUS'
																																																									                
																																																											                
																																																													               
																																																														                   }
																																																																           }
																																																																	           stage('Docker Build') {
																																																																		               steps {
																																																																			                       echo 'Docker'
																																																																					                       
																																																																							                       
																																																																									                      
																																																																											                  }
																																																																													          }
																																																																														          stage('Push Image to Dockerhub') {
																																																																															              steps {
																																																																																                      echo 'Docker Registry'
																																																																																		                      
																																																																																				                      
																																																																																						                     
																																																																																								                 }
																																																																																										         }
																																																																																											         stage('Deploy on Dev Environment') {
																																																																																												             steps {
																																																																																													                     echo 'Deploy on Pre-Production server'
																																																																																															                     
																																																																																																	                     
																																																																																																			                    
																																																																																																					                }
																																																																																																							        }
																																																																																																								        stage('Deploy on Pre-Prod Environment') {
																																																																																																									            steps {
																																																																																																										                    echo 'Deploy on Development server'
																																																																																																												                    
																																																																																																														                    
																																																																																																																                   
																																																																																																																		               }
																																																																																																																			               }
																																																																																																																				               stage('Deploy on Prod Environment') {
																																																																																																																					                   steps {
																																																																																																																							                   echo 'Deploy on Prod server'
																																																																																																																									                   
																																																																																																																											                   
																																																																																																																													                  
																																																																																																																															              }
																																																																																																																																              }
																																																																																																																																	              
																																																																																																																																		              
																																																																																																																																			          
																																																																																																																																				      }
																																																																																																																																				      }pipeline {
    agent any

    stages {
        stage('compile') {
            steps {
                echo 'compiling stage'
                git 'https://github.com/kwameindevops/DevOpsClassCodes-aug.git'
                sh 'mvn compile'
                
            }
        }
        stage('test') {
            steps {
                echo 'testing stage'
                sh 'mvn test'
              
                
            }
        }
        stage('package') {
            steps {
                echo 'package stage'
                sh 'mvn package'
                
               
            }
        }
        stage('Aqua-scan') {
            steps {
                echo 'Security'
                
                
               
            }
        }
        stage('publish to NEUS') {
            steps {
                echo 'Store package in NEXUS'
                
                
               
            }
        }
        stage('Docker Build') {
            steps {
                echo 'Docker'
                
                
               
            }
        }
        stage('Push Image to Dockerhub') {
            steps {
                echo 'Docker Registry'
                
                
               
            }
        }
        stage('Deploy on Dev Environment') {
            steps {
                echo 'Deploy on Pre-Production server'
                
                
               
            }
        }
        stage('Deploy on Pre-Prod Environment') {
            steps {
                echo 'Deploy on Development server'
                
                
               
            }
        }
        stage('Deploy on Prod Environment') {
            steps {
                echo 'Deploy on Prod server'
                
                
               
            }
        }
        
        
    
    }
